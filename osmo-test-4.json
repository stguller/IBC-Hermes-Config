[[chains]]
id = 'osmo-test-4'
rpc_addr = 'https://127.0.0.1:26557/'
websocket_addr = 'ws://127.0.0.1:26557/websocket'
grpc_addr = 'http://127.0.0.1:26557/'
rpc_timeout = '30s'
account_prefix = 'osmo'
key_name = 'osmosisrelayer'
store_prefix = 'ibc'
